{
  "name": "evergram-consumer-manager",
  "description": "Evergram's Instagram Consumer",
  "keywords": [
    "express",
    "mongoose",
    "mongodb"
  ],
  "version": "0.0.1",
  "private": true,
  "author": "Josh Stuart <joshstuartx@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/evergram/evergram-consumer-manager.git"
  },
  "engines": {
    "node": "0.12.00"
  },
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/nodemon server.js",
    "pretest": "npm run-script lint && npm run-script checkStyle",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --recursive -R spec -r should",
    "lint": "./node_modules/.bin/jshint ./app",
    "checkStyle": "./node_modules/.bin/jscs ./app"
  },
  "dependencies": {
    "async": "0.9.0",
    "body-parser": "1.8.2",
    "evergram-common": "git+ssh://git@github.com:evergram/evergram-common.git#master",
    "express": "4.9.0",
    "lodash": "^3.5.0",
    "method-override": "2.2.0",
    "moment": "^2.9.0",
    "newrelic": "^1.18.3",
    "q": "^1.2.0"
  },
  "devDependencies": {
    "istanbul": "^0.3.13",
    "jscs": "^1.12.0",
    "jshint": "^2.7.0",
    "mocha": "^2.2.4",
    "should": "^6.0.1",
    "sinon": "^1.14.1"
  }
}
